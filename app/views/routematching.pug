doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    title Search for similar routes
    style.
        | * {
        | background-color: #45523E;
        | }
        | body {
        | font-family: &apos;Inter&apos;, sans-serif;
        | }
        | a {
        | display: block;
        | padding: 8px;
        | background-color: #010101;
        | }
        | ul {
        | list-style-type: none;
        | margin: 0;
        | padding: 0;
        | overflow: hidden;
        | background-color: #010101;
        | }
        | li {
        | float: right;
        | }
        | li a {
        | display: block;
        | color: #F5ECE5;
        | text-align: center;
        | padding: 14px 16px;
        | text-decoration: none;
        | }
        | li a:hover {
        | background-color: #F5ECE5;
        | color: rgb(0, 0, 0);
        | }
        | a.current {
        | background-color: #B0ACA2;
        | color: #010101;
        | }
        | .add_image {
        | border-radius: 8px;
        | }
        | .bg {
        | display: flex;
        | flex-direction: column;
        | background-color: transparent;
        | }
        | .bottom_bg {
        | display: flex;
        | flex-direction: column;
        | background-color: #45523E;
        | height: 100%;
        | width: 100%;
        | }
        | .paragraph {
        | color: #F5ECE5;
        | }
        | .sized_box1 {
        | height: 20px;
        | }
        | .sized_box2 {
        | height: 10px;
        | background-color: #010101;
        | }
        | .rlp {
        | border-radius: 8px;
        | display: flex;
        | height: 200pt;
        | flex-direction: row;
        | justify-content: space-evenly;
        | align-items: center;
        | /* background-color: wheat; */
        | background-color: rgba(0, 0, 0, 0.2);
        | }
        | .rlpc1 {
        | padding: 10px;
        | box-sizing: border-box;
        | border-radius: 8px;
        | width: 20%;
        | height: 60%;
        | background-color: #F5ECE5;
        | justify-content: center;
        | align-items: center;
        | text-align: center;
        | }
        | .rlpc2 {
        | padding: 10px;
        | box-sizing: border-box;
        | border-radius: 8px;
        | width: 20%;
        | height: 60%;
        | background-color: #F5ECE5;
        | justify-content: center;
        | align-items: center;
        | text-align: center;
        | }
        | .rlpc3 {
        | padding: 10px;
        | box-sizing: border-box;
        | border-radius: 8px;
        | width: 20%;
        | height: 60%; background-color:#F5ECE5;
        | justify-content: center;
        | align-items: center;
        | text-align: center;
        | }
        | /* goto each page button*/
        | .button {
        | display: inline-block;
        | padding: 12px 24px;
        | font-size: 16px;
        | font-weight: bold;
        | text-align: center;
        | text-decoration: none;
        | background-color: #45523E;
        | background-image: linear-gradient(to bottom, #45523E, #516148);
        | color: #F5ECE5;
        | border-radius: 6px;
        | transition: background-color 0.3s;
        | }
        | .button:hover {
        | background-color: #45523E;
        | background-image: linear-gradient(to bottom, #113613, #072709);
        | }
        | .button_description {
        | background-color: #F5ECE5;
        | }
    // <link rel="stylesheet" href="style/routmatch_style.css">
  body
    nav
      .menu
        ul
          li Home
          li
            b Route matching
          li Long trip
          li Rent
          li Profile
    .container
      h1.findRoute Find Route
      form(action='routmatch_result.html', method='get')
        input(type='text', placeholder='start position', name='startPosition')
        input(type='text', placeholder='destination', name='destination')
        .formRow
          input(type='date', value='2017-06-01')
          button.findButton(type='submit')
            h4.findtext Find